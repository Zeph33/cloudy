{
  "name": "zhost",
  "version": "0.1.0",
  "description": "Node server, app build with Brunch using Vue, Vuex and VueRouter along with axios for client-server communication ",
  "main": "server.js",
  "scripts": {
    "watch:server": "nodemon server.js --ignore public/ --ignore app/",
    "watch:app": "brunch watch",
    "watch2": "npm run clean && npm-run-all --parallel watch:*",
    "watch": "npm run clean && brunch watch --server",
    "clean": "rimraf public/**",
    "build": "npm run clean && brunch build --production && rimraf public/test**",
    "start": "cross-env HOST=0.0.0.0 NODE_ENV=production node server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zeph33/zcloudy.git"
  },
  "keywords": [
    "vue",
    "brunch",
    "vuex",
    "vue-router"
  ],
  "author": "Zeph <zeph33@gmail.com>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/Zeph33/zcloudy/issues"
  },
  "homepage": "https://github.com/Zeph33/zcloudy",
  "devDependencies": {
    "auto-reload-brunch": "^2.7.1",
    "axios": "^0.17.1",
    "babel-brunch": "^6.0.6",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.22.0",
    "brunch": "^2.10.11",
    "clean-css-brunch": "^2.10.0",
    "copyfilemon-brunch": "^3.3.0",
    "css-loader": "^0.28.7",
    "dploy": "^1.2.0",
    "eslint": "^4.11.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "pug": "^2.0.0-rc.4",
    "pugjs-brunch": "^2.10.1",
    "rimraf": "^2.6.2",
    "store": "^2.0.12",
    "style-loader": "^0.19.0",
    "stylus": "^0.54.5",
    "stylus-brunch": "^2.10.0",
    "stylus-loader": "^3.0.1",
    "uglify-js-brunch": "^2.10.0",
    "vue-brunch": "^2.0.1",
    "vue-loader": "^13.5.0",
    "vue-template-compiler": "^2.5.8",
    "vueify": "^9.4.1"
  },
  "dependencies": {
    "cross-env": "^5.1.1",
