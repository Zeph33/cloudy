{
  "plugins": [
  ],
  "extends": [
    "plugin:vue/recommended"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "globals" :{
    "socket": true,
    "app": true,
    "Vue": true,
    "io": true
  },
  "rules": {
    "indent": [
      "warn",
      2
    ],
    "keyword-spacing": "off",
    "space-before-function-paren": "off",
    "no-const-assign": "warn",
    "no-this-before-super": "warn",
    "no-undef": "warn",
    "no-unreachable": "warn",
    "no-unused-vars": [
      "warn",
      {
        "vars": "local",
        "argsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^ignore",
        "args": "after-used"
      }
    ],
    "constructor-super": "warn",
    "valid-typeof": "warn"
  }
}